    "mocha": "^6.2.3",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.5.2",
    "ts-node-dev": "^1.1.1",
    "tslint": "^5.20.1",
    "typescript": "^4.1.2"
  }
}
